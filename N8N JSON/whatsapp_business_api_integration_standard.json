{
  "name": "WhatsApp Business API Integration (Standard Nodes)",
  "nodes": [
    {
      "parameters": {
        "authentication": "genericCredentialType",
        "genericAuthType": "text",
        "url": "=https://{{$env.WHATSAPP_API_DOMAIN}}/v1/webhooks/incoming",
        "responseMode": "responseNode"
      },
      "id": "a1b2c3d4-e5f6-7a8b-9c0d-1e2f3a4b5c6d",
      "name": "WhatsApp Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        240,
        300
      ],
      "webhookId": "whatsapp-business-webhook"
    },
    {
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "string": [
            {
              "name": "customerNumber",
              "value": "={{ $json.entry[0].changes[0].value.messages[0].from }}"
            },
            {
              "name": "businessNumber",
              "value": "={{ $json.entry[0].changes[0].value.metadata.display_phone_number }}"
            },
            {
              "name": "messageContent",
              "value": "={{ $json.entry[0].changes[0].value.messages[0].text.body }}"
            },
            {
              "name": "messageId",
              "value": "={{ $json.entry[0].changes[0].value.messages[0].id }}"
            },
            {
              "name": "timestamp",
              "value": "={{ $json.entry[0].changes[0].value.messages[0].timestamp }}"
            }
          ],
          "object": [
            {
              "name": "rawData",
              "value": "={{ $json }}"
            }
          ]
        }
      },
      "id": "b1c2d3e4-f5g6-h7i8-j9k0-l1m2n3o4p5q6",
      "name": "Extract WhatsApp Data",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [
        460,
        300
      ]
    },
    {
      "parameters": {
        "operation": "formatDate",
        "date": "={{ new Date(parseInt($json.timestamp) * 1000) }}",
        "format": "MMMM DD, YYYY (h:mma)",
        "outputFieldName": "formattedTimestamp"
      },
      "id": "c9d8e7f6-g5h4-i3j2-k1l0-m9n8o7p6q5r4",
      "name": "Format Timestamp",
      "type": "n8n-nodes-base.dateTime",
      "typeVersion": 1,
      "position": [
        640,
        300
      ]
    },
    {
      "parameters": {
        "operation": "find",
        "collection": "services",
        "where": [
          {
            "field": "whatsappNumber",
            "operator": "==",
            "value": "={{ $json.businessNumber }}"
          },
          {
            "field": "status",
            "operator": "==",
            "value": "active"
          }
        ],
        "options": {
          "limit": 1
        }
      },
      "id": "c1d2e3f4-g5h6-i7j8-k9l0-m1n2o3p4q5r6",
      "name": "Find Service by WhatsApp Number",
      "type": "n8n-nodes-base.googleFirestore",
      "typeVersion": 2,
      "credentials": {
        "googleFirestoreOAuth2Api": {
          "id": "google-firestore-credentials",
          "name": "Google Firestore Account"
        }
      },
      "position": [
        820,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "number": [
            {
              "value1": "={{ $json.length }}",
              "operation": "larger",
              "value2": 0
            }
          ]
        }
      },
      "id": "d1e2f3g4-h5i6-j7k8-l9m0-n1o2p3q4r5s6",
      "name": "IF (Service Found)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        1000,
        300
      ]
    },
    {
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "boolean": [
            {
              "name": "serviceFound",
              "value": true
            }
          ],
          "string": [
            {
              "name": "serviceId",
              "value": "={{ $json[0].id }}"
            },
            {
              "name": "serviceType",
              "value": "={{ $json[0].type }}"
            },
            {
              "name": "businessName",
              "value": "={{ $json[0].businessName }}"
            },
            {
              "name": "aiSystemPrompt",
              "value": "={{ $json[0].aiSystemPrompt }}"
            }
          ]
        }
      },
      "id": "e1f2g3h4-i5j6-k7l8-m9n0-o1p2q3r4s5t6",
      "name": "Extract Service Data",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [
        1160,
        240
      ]
    },
    {
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "boolean": [
            {
              "name": "serviceFound",
              "value": false
            }
          ],
          "string": [
            {
              "name": "serviceId",
              "value": ""
            },
            {
              "name": "serviceType",
              "value": ""
            },
            {
              "name": "businessName",
              "value": "SynchroAI"
            },
            {
              "name": "aiSystemPrompt",
              "value": ""
            }
          ]
        }
      },
      "id": "f1g2h3i4-j5k6-l7m8-n9o0-p1q2r3s4t5u6",
      "name": "Set Default Values",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [
        1160,
        400
      ]
    },
    {
      "parameters": {
        "dataType": "string",
        "value1": "={{ $json.serviceFound }}",
        "rules": {
          "rules": [
            {
              "value2": true,
              "outputValue": "={{ $json.aiSystemPrompt }}"
            }
          ]
        },
        "fallbackValue": "I'm sorry, but this WhatsApp number is not associated with an active service. Please contact the service provider for assistance."
      },
      "id": "g1h2i3j4-k5l6-m7n8-o9p0-q1r2s3t4u5v6",
      "name": "Determine System Prompt",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        1320,
        300
      ]
    },
    {
      "parameters": {
        "url": "=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent?key={{$env.GEMINI_API_KEY}}",
        "method": "POST",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "{\n  \"contents\": [\n    {\n      \"role\": \"user\",\n      \"parts\": [\n        {\n          \"text\": \"{{ $node[\"Extract WhatsApp Data\"].json.messageContent }}\"\n        }\n      ]\n    }\n  ],\n  \"systemInstruction\": {\n    \"parts\": [\n      {\n        \"text\": \"{{ $json }}\"\n      }\n    ]\n  },\n  \"generationConfig\": {\n    \"temperature\": 0.2,\n    \"maxOutputTokens\": 1024,\n    \"topK\": 40,\n    \"topP\": 0.95\n  }\n}",
        "options": {
          "response": {
            "fullResponse": true
          }
        }
      },
      "id": "h1i2j3k4-l5m6-n7o8-p9q0-r1s2t3u4v5w6",
      "name": "Generate AI Response",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [
        1500,
        300
      ]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "aiResponse",
              "value": "={{ $json.body.candidates[0].content.parts[0].text }}"
            }
          ]
        },
        "options": {
          "dotNotation": true
        }
      },
      "id": "i1j2k3l4-m5n6-o7p8-q9r0-s1t2u3v4w5x6",
      "name": "Extract AI Response",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [
        1680,
        300
      ]
    },
    {
      "parameters": {
        "resource": "message",
        "operation": "send",
        "phoneNumberId": "={{ $node[\"Extract Message Data\"].json.whatsappBusinessAccountId }}",
        "target": "{{ $node[\"Extract Message Data\"].json.customerNumber }}",
        "targetType": "individual",
        "messageType": "text",
        "text": "={{ $node[\"Extract AI Response\"].json.aiResponse }}",
        "additionalFields": {
          "previewUrl": false
        }
      },
      "id": "j1k2l3m4-n5o6-p7q8-r9s0-t1u2v3w4x5y6",
      "name": "Send WhatsApp Response",
      "type": "n8n-nodes-base.whatsapp",
      "typeVersion": 1,
      "credentials": {
        "whatsAppApi": {
          "id": "whatsapp-api-credentials",
          "name": "WhatsApp API"
        }
      },
      "position": [
        1860,
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $node[\"Extract Service Data\"].json.serviceType }}",
              "operation": "equals",
              "value2": "whatsapp-order"
            }
          ]
        }
      },
      "id": "l1m2n3o4-p5q6-r7s8-t9u0-v1w2x3y4z5a6",
      "name": "IF (Order Service)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        2220,
        300
      ]
    },
    {
      "parameters": {
        "fields": {
          "values": [
            {
              "name": "hasOrderIntent",
              "type": "boolean",
              "parameterValue": "={{ $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"order\") || $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"buy\") || $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"purchase\") || $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"checkout\") || $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"payment\") || $node[\"Extract WhatsApp Data\"].json.messageContent.toLowerCase().includes(\"pay\") }}"
            }
          ]
        },
        "options": {}
      },
      "id": "m1n2o3p4-q5r6-s7t8-u9v0-w1x2y3z4a5b6",
      "name": "Detect Order Intent",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [
        2400,
        240
      ]
    },
    {
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{ $json.hasOrderIntent }}",
              "value2": true
            }
          ]
        }
      },
      "id": "n1o2p3q4-r5s6-t7u8-v9w0-x1y2z3a4b5c6",
      "name": "IF (Has Order Intent)",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [
        2580,
            },
            "options": {
              "dotNotation": true
            }
          },
          "id": "i1j2k3l4-m5n6-o7p8-q9r0-s1t2u3v4w5x6",
          "name": "Extract AI Response",
          "type": "n8n-nodes-base.set",
          "typeVersion": 3,
          "position": [
            1680,
            300
          ]
      "typeVersion": 2,
      "position": [
        2760,
        180
      ]
    },
    {
      "parameters": {
        "authentication": "genericCredentialType",
        "genericAuthType": "basicAuth",
        "genericAuthTypeBasicAuth": {
          "user": "={{ $env.SMTP_USERNAME }}",
          "password": "={{ $env.SMTP_PASSWORD }}"
        },
        "fromEmail": "={{ $env.NOTIFICATION_EMAIL }}",
        "toEmail": "={{ $env.ADMIN_EMAIL }}",
        "subject": "=New Order Received - {{ $node[\"Extract Service Data\"].json.businessName }} - {{ $node[\"Format Timestamp\"].json.formattedTimestamp }}",
        "html": "<h2>New Order Received</h2>\n<p><strong>Business:</strong> {{ $node[\"Extract Service Data\"].json.businessName }}</p>\n<p><strong>Received:</strong> {{ $node[\"Format Timestamp\"].json.formattedTimestamp }}</p>\n<p><strong>From:</strong> {{ $node[\"Extract WhatsApp Data\"].json.customerNumber }}</p>\n\n<h3>Order Details:</h3>\n<p>{{ $node[\"Extract WhatsApp Data\"].json.messageContent }}</p>\n\n<h3>AI Response:</h3>\n<p>{{ $node[\"Extract AI Response\"].json.aiResponse }}</p>\n\n<p>Please sign in to the SynchroAI dashboard to process this order.</p>\n\n<p><em>This is an automated notification from SynchroAI.</em></p>"
      },
      "id": "p1q2r3s4-t5u6-v7w8-x9y0-z1a2b3c4d5e6",
      "name": "Send Order Notification",
      "type": "n8n-nodes-base.emailSend",
      "typeVersion": 1,
      "position": [
        2760,
        320
      ]
    }
  ],
